{
  "name": "doclink",
  "version": "1.0.0",
  "description": "Doc Link Input",
  "scripts": {
    "start": "esbuild ./src/URLChangerNew.tsx --outfile=./build/urlchanger_ng.js --sourcemap=both --serve=8080 --servedir=build --bundle --format=esm --watch",
    "build": "esbuild ./src/URLChangerNew.tsx --outfile=./build/urlchanger_ng.js --bundle  --format=esm",
    "startLegacy": "esbuild ./src/URLChangerLegacy.tsx --outfile=./build/urlchanger_leg.js --sourcemap=both --serve=8080 --servedir=build --bundle --format=esm --watch",
    "buildLegacy": "esbuild ./src/URLChangerLegacy.tsx --outfile=./build/urlchanger_leg.js --bundle  --format=esm ",
    "buildAll": "npm run build & npm run buildLegacy"
  },
  "author": "Boomi",
  "license": "MIT",
  "dependencies": {
    "fcmlib": "^1.0.87",
    "react": "18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.28",
    "esbuild": "0.19.4"
  }
}
